<div id="app"></div>

<script>
  $(function() {
    var promises = [
      App.WorldName.find({}),
      App.EventName.find({}),
      App.MapName.find({}),
      App.Continent.find({})
    ];

    Ember.RSVP.all(promises).then(function() {
      App.advanceReadiness();
    });
  });
</script>
